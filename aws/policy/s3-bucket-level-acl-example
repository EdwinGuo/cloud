vault write aws/role/eddy-s3 policy=-<<EOF{\n    "Version": "2012-10-17",\n    "Statement": [\n        {\n            "Effect": "Allow",\n            "Action": [\n                "s3:GetBucketLocation",\n                "s3:ListAllMyBuckets"\n            ],\n            "Resource": "arn:aws:s3:::*"\n        },\n        {\n            "Effect": "Allow",\n            "Action": [\n                "s3:*"\n            ],\n            "Resource": [\n                "arn:aws:s3:::eddytestpolicy"\n            ]\n        },\n        {\n            "Effect": "Allow",\n            "Action": [\n                "s3:PutObject",\n                "s3:GetObject",\n                "s3:DeleteObject"\n            ],\n            "Resource": [\n                "arn:aws:s3:::eddytestpolicy/*"\n            ]\n        }\n    ]\n}\nEOF\n\n\n\n
